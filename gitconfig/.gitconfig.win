[user]
	name = liliang
	email = liliangneu@126.com
[filter "lfs"]
	clean = git lfs clean %f
	smudge = git lfs smudge %f
	required = true
[color]
	ui = true
[alias]
	s = status
	c = checkout
	b = branch
	lol = log --oneline --graph --decorate

[merge]
	tool = meld
[mergetool "meld"]
	cmd = meld.exe $BASE $LOCAL $REMOTE $MERGED
	trustExitCode = false
[diff]
	tool = meld
[difftool "meld"]
	cmd = meld.exe $LOCAL $REMOTE
[gui]
	recentrepo = E:/imdb/Config
